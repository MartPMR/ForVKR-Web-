@model IEnumerable<string>

<div data-role="navbar">
    <ul>
        @foreach (var link in Model)
        {
            <li>
                @Html.RouteLink(link, new
           {
               controller = "Books",
               action = "List",
               genre = link,
               page = 1
           }, new
           {
               data_transition = "fade",
               @class = (link == ViewBag.SelectedNameGenre ? "ui-btn-active" : null)
           })
            </li>
        }
    </ul>
</div>